<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <filter>
        <filter-name>FilterDispatcher</filter-name>
        <filter-class>loclt.filter.FilterDispatcher</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>FilterDispatcher</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <listener>
        <description>ServletContextListener</description>
        <listener-class>loclt.filter.ContextListen</listener-class>
    </listener>
    <servlet>
        <servlet-name>DBUtils</servlet-name>
        <servlet-class>loclt.utils.DBUtils</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>loclt.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>loclt.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserVerifyServlet</servlet-name>
        <servlet-class>loclt.servlet.UserVerifyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateNewAccountServlet</servlet-name>
        <servlet-class>loclt.servlet.CreateNewAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyAccountServlet</servlet-name>
        <servlet-class>loclt.servlet.VerifyAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminManagementCarServlet</servlet-name>
        <servlet-class>loclt.servlet.AdminManagementCarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRentalCarServlet</servlet-name>
        <servlet-class>loclt.servlet.UserRentalCarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserGoShoppingServlet</servlet-name>
        <servlet-class>loclt.servlet.UserGoShoppingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserSearchCarsServlet</servlet-name>
        <servlet-class>loclt.servlet.UserSearchCarsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAddCarsShoppingServlet</servlet-name>
        <servlet-class>loclt.servlet.UserAddCarsShoppingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserTakeOverCartServlet</servlet-name>
        <servlet-class>loclt.servlet.UserTakeOverCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserOrderCarsServlet</servlet-name>
        <servlet-class>loclt.servlet.UserOrderCarsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserViewDetailsHistoryServlet</servlet-name>
        <servlet-class>loclt.servlet.UserViewDetailsHistoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserViewHistoryServlet</servlet-name>
        <servlet-class>loclt.servlet.UserViewHistoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DBUtils</servlet-name>
        <url-pattern>/DBUtils</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateNewAccountServlet</servlet-name>
        <url-pattern>/CreateNewAccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyAccountServlet</servlet-name>
        <url-pattern>/VerifyAccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminManagementCarServlet</servlet-name>
        <url-pattern>/AdminManagementCarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRentalCarServlet</servlet-name>
        <url-pattern>/UserRentalCarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserGoShoppingServlet</servlet-name>
        <url-pattern>/UserGoShoppingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserSearchCarsServlet</servlet-name>
        <url-pattern>/UserSearchCarsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAddCarsShoppingServlet</servlet-name>
        <url-pattern>/UserAddCarsShoppingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserTakeOverCartServlet</servlet-name>
        <url-pattern>/UserTakeOverCartServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserOrderCarsServlet</servlet-name>
        <url-pattern>/UserOrderCarsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserViewDetailsHistoryServlet</servlet-name>
        <url-pattern>/UserViewDetailsHistoryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserViewHistoryServlet</servlet-name>
        <url-pattern>/UserViewHistoryServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
